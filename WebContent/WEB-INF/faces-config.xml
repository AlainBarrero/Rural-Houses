<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>QueryAvailability</managed-bean-name>
		<managed-bean-class>beans.QueryAvailabilityBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>BookBean</managed-bean-name>
		<managed-bean-class>beans.BookRuralHouseBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>setAval</managed-bean-name>
		<managed-bean-class>beans.SetAvailability</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>RuralHouse.xhtml</display-name>
		<from-view-id>/RuralHouse.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>act1</from-outcome>
			<to-view-id>/QueryAvailability.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>RuralHouse.xhtml</display-name>
		<from-view-id>/RuralHouse.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>act2</from-outcome>
			<to-view-id>/SetAvailability.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>RuralHouse.xhtml</display-name>
		<from-view-id>/RuralHouse.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>act3</from-outcome>
			<to-view-id>/BookRuralHouse.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>BookRuralHouse.xhtml</display-name>
		<from-view-id>/BookRuralHouse.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>act5</from-outcome>
			<to-view-id>/BookHouseConfirmation.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>QueryAvailability.xhtml</display-name>
		<from-view-id>/QueryAvailability.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>act6</from-outcome>
			<to-view-id>/RuralHouse.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>SetAvailabilityHouse.xhtml</display-name>
		<from-view-id>/SetAvailabilityHouse.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>act7</from-outcome>
			<to-view-id>/RuralHouse.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>BookRuralHouse.xhtml</display-name>
		<from-view-id>/BookRuralHouse.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>act8</from-outcome>
			<to-view-id>/RuralHouse.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>BookHouseConfirmation.xhtml</display-name>
		<from-view-id>/BookHouseConfirmation.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>act9</from-outcome>
			<to-view-id>/RuralHouse.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>SetAvailability.xhtml</display-name>
		<from-view-id>/SetAvailability.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>ok</from-outcome>
			<to-view-id>/SetAvailabilityHouse.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
